{
  "packageManager": "yarn@3.1.0",
  "scripts": {
    "test": "NODE_ENV=test jest --rootDir ./src/backend",
    "dev": "nodemon --exec babel-node ./src/backend/server.js",
    "start": "babel-node ./src/backend/server.js",
    "init-db": "babel-node ./src/scripts/init-db-and-mock-data.js",
    "build": "rm -rf ./dist && babel ./src --out-dir ./dist --copy-files",
    "prod": "node ./dist/server.js",
    "prod-init-db": "node ./dist/scripts/init-db-and-mock-data.js",
    "prod-test": "NODE_ENV=test jest --rootDir ./dist/backend"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-validator": "^6.13.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.12.0-alpha.1",
    "jest": "^27.3.1",
    "supertest": "^6.1.6",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/runtime": "^7.16.3",
    "@babel/cli": "^7.16.0",
    "@babel/plugin-transform-modules-commonjs": "^7.16.0",
    "babel-jest": "^27.3.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1",
    "slash": "^4.0.0"
  }
}